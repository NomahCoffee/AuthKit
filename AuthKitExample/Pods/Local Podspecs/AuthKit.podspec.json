  "homepage": "https://github.com/NomahCoffee/AuthKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Caleb Rudnicki": "calebrudnicki@gmail.com"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/NomahCoffee/AuthKit.git",
    "tag": "1.2"
